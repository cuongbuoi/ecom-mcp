{
  "name": "kudosi-mcp",
  "version": "1.0.0",
  "description": "KudosiMCP is an MCP server to interact with Kudosi app",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "npx fastmcp dev src/index.ts",
    "inspect": "npx fastmcp inspect src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "fastmcp": "^1.22.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
