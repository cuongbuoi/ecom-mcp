{
  "name": "ecom-mcp",
  "version": "1.0.0",
  "description": "FastMCP server for collecting AliExpress product reviews",
  "type": "module",
  "scripts": {
    "build": "tsc && chmod 755 build/index.js",
    "start": "node build/index.js",
    "dev": "npx fastmcp dev build/index.js",
    "inspect": "npx fastmcp inspect build/index.js",
    "example:basic": "npx tsx examples/basic-server.ts",
    "example:advanced": "npx tsx examples/advanced-server.ts",
    "example:sse": "npx tsx examples/sse-server.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "bin": {
    "ecom-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "dotenv": "^16.5.0",
    "fastmcp": "^1.22.2",
    "puppeteer": "^24.6.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.2.4",
    "ts-jest": "^29.3.2",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
